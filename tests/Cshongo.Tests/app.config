<?xml version="1.0"?>
<configuration>
	<connectionStrings>
		<add name="MongoDB_Master" connectionString="mongodb://localhost"/>
	</connectionStrings>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
	</startup>
	<appSettings>
		<!-- Mongo database settings -->
		<add key="MongoDB_MasterDB_Name" value="CongoTests"/>
		<add key="MongoDB_MasterDB_UserName" value=""/>
		<add key="MongoDB_MasterDB_Password" value=""/>
	</appSettings>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="System.Web.Mvc" publicKeyToken="31bf3856ad364e35" culture="neutral"/>
				<bindingRedirect oldVersion="0.0.0.0-4.0.0.0" newVersion="4.0.0.0"/>
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
</configuration>
